{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "alperenersoy/filament-export": "^0.3.2",
        "althinect/filament-spatie-roles-permissions": "^1.2",
        "awcodes/filament-quick-create": "^2.1",
        "awcodes/filament-versions": "^1.1",
        "brickx/laravel-filament-maintenance": "^1.1",
        "calebporzio/sushi": "^2.4",
        "camya/filament-title-with-slug": "^0.5.6",
        "doctrine/dbal": "^3.6",
        "filament/filament": "^2.0",
        "filament/spatie-laravel-media-library-plugin": "^2.0",
        "fruitcake/laravel-cors": "^2.0.5",
        "guzzlehttp/guzzle": "^7.2",
        "kenepa/multi-widget": "^1.0",
        "konnco/filament-import": "^1.5",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.14",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^2.12",
        "malzariey/filament-daterangepicker-filter": "1.3.1.2",
        "nesbot/carbon": "^2.66",
        "ralphjsmit/tall-interactive": "^0.9.0",
        "shuvroroy/filament-spatie-laravel-backup": "^1.3",
        "shuvroroy/filament-spatie-laravel-health": "^1.6",
        "spatie/laravel-permission": "^5.10",
        "suleymanozev/filament-radio-button-field": "^2.0",
        "yepsua/filament-captcha-field": "^0.1.1",
        "yepsua/filament-themes": "^0.2.1",
        "z3d0x/filament-logger": "^0.5.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "SQ\\": "sq/lib/"
        },
        "files": ["sq/sq.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "ext-pcntl": "8.0",
            "ext-posix": "8.0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "post-update-cmd": [
        "@php artisan filament:upgrade"
    ]
}
